        "# sort\n",
        "def sort_data(x, y):\n",
        "  xy = [(xi, yi) for xi, yi in zip(x, y)]\n",
        "  sorted_xy = sorted(xy, key=lambda item: item[0])\n",
        "  print(sorted_xy)\n",
        "\n",
        "  return [xi for xi, _ in sorted_xy], [yi for _, yi in sorted_xy]\n",
        "\n",
        "x, y = sort_data(x, y)\n",
        "\n",
        "plt.figure(figsize=(8, 6))\n",
        "plt.scatter(x[0:15], y[0:15])\n",
        "plt.show()"
